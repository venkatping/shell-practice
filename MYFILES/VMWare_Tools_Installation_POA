cd /tmp/vmware-tools-distrib
./vmware-install.pl
vmtoolsd -vv
./vmware-install.pl
vmtoolsd -vv
/etc/init.d/vmware-tools status
vmtoolsd -vv
/bin/vmtoolsd -vv
vmtoolsd -vv
vmtoolsd -v
/etc/init.d/vmware-tools status
vmtoolsd -vv
init 6 # Post Vmware tools installation.
